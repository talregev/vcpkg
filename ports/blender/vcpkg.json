{
  "name": "blender",
  "version": "4.5.3",
  "description": "3D creation suite",
  "homepage": "https://www.blender.org/",
  "license": "GPL-1.0-or-later",
  "supports": "static",
  "dependencies": [
    "libjpeg-turbo",
    "zlib",
    "libpng",
    "zstd",
    "libepoxy",
    "tiff",
    "liblzma",
    "bzip2",
    "pthreads",
    {
      "name": "openimageio",
      "features": [ 
        "opencolorio"
      ]
    },
    "freetype",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ]
}
